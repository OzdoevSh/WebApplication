@model IEnumerable<Query6Result>
@{
    ViewBag.Title = "Результат запроса";
}

<h2>Результат запроса</h2>

<table class="table table-bordered table-hover">
    <tr>
        <th>Название</th>
        <th>Цена</th>
        <th>Количество</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.name</td>
            <td>@item.price</td>
            <td>@item.count</td>
        </tr>
    }

</table>