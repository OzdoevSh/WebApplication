@model IEnumerable<Query3Result>
@{
    ViewBag.Title = "Результат запроса";
}

<h2>Результат запроса</h2>

<table class="table table-bordered table-hover">
    <tr>
        <th>Название</th>
        <th>Номер</th>
        <th>Общая стоимость</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.id</td>
            <td>@item.name</td>
            <td>@item.summary</td>
        </tr>
    }

</table>
