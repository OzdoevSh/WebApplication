@model IEnumerable<WebApplication.Models.category>

@{
    ViewBag.Title = "Search";
}

<h2>Результаты поиска</h2>

<table class="table">
    <tr>
        <th>Name</th>
        <th>ParentName</th>
        <th></th>
    </tr>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.category2.name)
        </td>
        <td>
            @Html.ActionLink("Правка", "Edit", new { id=item.id }) |
            @Html.ActionLink("Подробности", "Details", new { id=item.id }) |
            @Html.ActionLink("Удалить", "Delete", new { id=item.id })
        </td>
    </tr>
}
</table>